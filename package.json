{
  "name": "mockjs-lite",
  "version": "0.3.0",
  "description": "A minimalist simulation data generator",
  "main": "dist/mock.js",
  "scripts": {
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production",
    "test": "nyc ava",
    "test-cov": "nyc --reporter=html --reporter=text ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/52cik/mockjs-lite.git"
  },
  "keywords": ["mock", "mockjs"],
  "author": "楼教主 <fe.52cik@gmail.com> (http://www.52cik.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/52cik/mockjs-lite/issues"
  },
  "homepage": "http://mockjs-lite.js.org",
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "nyc": "^11.1.0",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1"
  },
  "engines": {
    "node": ">=4"
  }
